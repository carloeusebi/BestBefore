{
    "build": {
        "development": {
            "distribution": "internal",
            "developmentClient": true,
            "android": {
                "buildType": "apk"
            },
            "env": {
                "APP_VARIANT": "development"
            }
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "APP_VARIANT": "preview"
            }
        },
        "production": {
            "android": {
                "buildType": "app-bundle"
            }
        }
    }
}
