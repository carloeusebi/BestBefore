includes:
    - vendor/larastan/larastan/extension.neon
    - vendor/nesbot/carbon/extension.neon

parameters:
    paths:
        - app
        - bootstrap
        - database/factories
        - routes
        - config
    level: 8
    checkOctaneCompatibility: true
    checkModelProperties: true
    treatPhpDocTypesAsCertain: false

    ignoreErrors:

    excludePaths:
        - '**/ide-helper.php'
        - '**/ide_helper_models.php'
        - '**/phpstorm.meta.php'
